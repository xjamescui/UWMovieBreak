{
	"baseHtml":[
			"<div class='row uw_MB_mainTitle'>",
				"<h2 id='uw_MB_header' class='uw_MB_paddingLeft'><i class='fa fa-play-circle fa-lg'></i>  UW Movie Break</h2>",
				"<div id='break'></div>",
			"</div>",
			"<div id='uw_MB_search' class='row'></div>",
			"<div id='uw_MB_results' class='row'></div>",
			"<div id='uw_MB_details' class='row'></div>"
	],

	"search": [
		"<div class='container-fluid'>",
			"<div class='row'>",
				"<div class='col-xs-10 col-md-10 uw_MB_noPadding'>",
					"<div class='container-fluid'>",
						"<div class='row uw_MB_paddingTopBottom'>",
							"<span class='uw_MB_viewTitle'>Find a movie</span>",
						"</div>",
						"<div class='row uw_MB_redFill uw_MB_whiteText uw_MB_labelTitle'>",
							"<span class='uw_MB_paddingLeft'>Genre</span>",
						"</div>",
						"<div class='row uw_MB_paddingTopBottom uw_MB_paddingLeft uw_MB_lato'>",
							"<select id='uw_MB_genre'>",
								"{{#genres}}",
									"<option value={{id}}>{{name}}</option>",
								"{{/genres}}",
							"</select>",
						"</div>",
						"<div class='row uw_MB_redFill uw_MB_whiteText uw_MB_labelTitle'>",
							"<span class='uw_MB_paddingLeft'>Release Date</span>",
						"</div>",
						"<div class='row uw_MB_paddingTopBottom uw_MB_paddingLeft uw_MB_lato'>",
							"<input type='text' id='datepicker' name='releaseDate' value='Click to select'>",
						"</div>",
						"<div class='row uw_MB_paddingTopBottom uw_MB_alignCenter uw_MB_lato'>",
							"<button id='uw_MB_searchButton'>Search</button>",
						"</div>",
					"</div>",
				"</div>",
				"<div class='col-xs-2 col-md-2 uw_MB_noPadding'>",
					"<img src=images/popcorn.jpg class='img-responsive uw_MB_paddingLeft'>",
				"</div>",
			"</div>",
		"</div>"
	],

	"results": [
		"<div class='container-fluid'>",
			"<div class='row uw_MB_paddingTopBottom'>",
				"<div class='col-xs-4 col-md-4 uw_MB_noPadding'>",
					"<span class='uw_MB_viewTitle'>Results</span>",
				"</div>",
				"<div class='col-xs-8 col-md-8 uw_MB_noPadding uw_MB_backSearch'>",
					"<button id='uw_MB_backToSearch'>Back to search</button>",
				"</div>",
			"</div>",
			"{{#movies}}",
				"<div class='row uw_MB_result_item uw_MB_movieTitle uw_MB_clickable' data-value='{{id}}'>",
					"{{title}}",
				"</div>",
					"<div class='row uw_MB_lato uw_MB_marginBottom uw_MB_movieResultBackground'>",
						"<div class='col-xs-5 col-md-5 uw_MB_imgPadding'>",
							"<img src='http://image.tmdb.org/t/p/w500/{{poster_path}}' class='img-responsive'>",
						"</div>",
						"<div class='col-xs-7 col-md-7 uw_MB_noPadding uw_MB_paddingTopBottom'>",
						"</div>",
					"</div>",
			"{{/movies}}",
		"</div>"
	],

	"details": [
		"<div class='container-fluid'>",
			"<div class='row uw_MB_paddingTopBottom'>",
				"<span class='uw_MB_viewTitle'>Details</span>",
			"</div>",
			"<div class='row'>",
				"<div class='uw_MB_content_title'>{{original_title}}</div>",
				"<p> Release: {{release_date}} </p>",
				"<p> Runtime: {{runtime}} min. </p>",
			"</div>",
			"<div class='row uw_MB_paddingTopBottom uw_MB_alignCenter uw_MB_lato'>",
				"<button id='uw_MB_backToResults'>Back to results</button>",
			"</div>",
		"</div>"
	]
}
