{
	"baseHtml":[
			"<div class='row mainTitle'>",
				"<h2 id='uw_MB_header' class='paddingLeft'><i class='fa fa-play-circle fa-lg'></i>  UW Movie Break</h2>",
				"<div id='break'></div>",
			"</div>",
			"<div id='uw_MB_search' class='row'></div>",
			"<div id='uw_MB_results' class='row'></div>"
	],

	"search": [
		"<div class='container-fluid'>",
			"<div class='row'>",
				"<div class='col-xs-10 col-md-10 noPadding'>",
					"<div class='container-fluid'>",
						"<div class='row paddingTopBottom'>",
							"<span class='viewTitle'>Find a movie</span>",
						"</div>",
						"<div class='row redFill whiteText labelTitle'>",
							"<span class='paddingLeft'>Genre</span>",
						"</div>",
						"<div class='row paddingTopBottom paddingLeft lato'>",
							"<select id='uw_MB_genre'>",
								"{{#genres}}",
									"<option value={{id}}>{{name}}</option>",
								"{{/genres}}",
							"</select>",
						"</div>",
						"<div class='row redFill whiteText labelTitle'>",
							"<span class='paddingLeft'>Release Date</span>",
						"</div>",
						"<div class='row paddingTopBottom paddingLeft lato'>",
							"<input type='text' id='datepicker' name='releaseDate' value='Click to select'>",
						"</div>",
						"<div class='row paddingTopBottom alignCenter lato'>",
							"<button id='uw_MB_searchButton'>Search</button>",
						"</div>",
					"</div>",
				"</div>",
				"<div class='col-xs-2 col-md-2 noPadding'>",
					"<img src=images/popcorn.jpg class='img-responsive paddingLeft'>",
				"</div>",
			"</div>",
		"</div>"
	],

	"results": [
		"<div class='container-fluid'>",
			"<div class='row paddingTopBottom'>",
				"<span class='viewTitle'>Results</span>",
			"</div>",
				"{{#movies}}",
					"<div class='row movieTitle'>",
						"{{title}}",
					"</div>",
					"<div class='row lato paddingBottom'>",
						"<div class='col-xs-4 col-md-4 noPadding'>",
							"<img src='http://image.tmdb.org/t/p/w500/{{poster_path}}' class='img-responsive'>",
						"</div>",
						"<div class='col-xs-8 col-md-8 noPadding'>",

						"</div>",
					"</div>",
				"{{/movies}}",
		"</div>"
	],

	"details": []
}
